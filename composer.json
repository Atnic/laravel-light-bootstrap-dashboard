{
    "name": "atnic/laravel-light-bootstrap-dashboard",
    "description": "The Laravel Framework.",
    "keywords": ["laravel", "light-bootstrap-dashboard", "package", "composer"],
    "license": "MIT",
    "type": "package",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "^5.4"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7",
        "laravel/tinker": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Atnic\\LightBootstrapDashboard\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Atnic\\LightBootstrapDashboard\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Atnic\\LightBootstrapDashboard\\Providers\\AppServiceProvider"
            ]
        }
    }
}
